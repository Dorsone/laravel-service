{
    "name": "dorsone/laravel-service-generator",
    "type": "library",
    "license": "MIT",
    "description": "With this library, you can generate Services and automatically connect them to Controllers",
    "autoload": {
        "psr-4": {
            "Dorsone\\LaravelService\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jasur Dustmurodov",
            "email": "jasur.coder@gmail.com"
        },
        {
            "name": "Firdavs Ibodullaev",
            "email": "firdavs3253@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/console": "^8.0",
        "illuminate/support": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dorsone\\LaravelService\\Providers\\ServiceGeneratorProvider"
            ]
        }
    }
}
